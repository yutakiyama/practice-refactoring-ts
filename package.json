{
  "name": "practice-refactoring-ts",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev --respawn src/index.ts",
    "tsc": "tsc",
    "clean": "rimraf dist/*",
    "build": "npm-run-all clean tsc",
    "start": "node .",
    "check-types": "tsc --noEmit",
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write 'src/**/*.{js,ts,json}'",
    "lint": "npm-run-all eslint check-types",
    "lint:fix": "npm-run-all eslint:fix check-types format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yutakiyama/practice-refactoring-ts.git"
  },
  "keywords": [],
  "author": "yutakiyama",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yutakiyama/practice-refactoring-ts/issues"
  },
  "homepage": "https://github.com/yutakiyama/practice-refactoring-ts#readme",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
